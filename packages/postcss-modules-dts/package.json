{
  "name": "@guanghechen/postcss-modules-dts",
  "version": "3.0.0-alpha.0",
  "description": "Generate `*.d.ts` files for style files (such as css, stylus and etc)",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/guanghechen.git",
    "directory": "packages/postcss-modules-dts"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/release-3.x.x/packages/postcss-modules-dts#readme",
  "keywords": [
    "postcss",
    "typescript",
    "css",
    "d.ts"
  ],
  "type": "module",
  "exports": "./lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "echo 'no test needed.'"
  },
  "dependencies": {
    "@guanghechen/helper-option": "^3.0.0-alpha.0",
    "@types/reserved-words": "0.1.0",
    "fs-extra": "^11.1.0",
    "reserved-words": "0.1.2"
  }
}
