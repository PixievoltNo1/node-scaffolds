{
  "name": "@guanghechen/postcss-modules-dts",
  "version": "1.0.7",
  "description": "Generate `*.d.ts` files for style files (such as css, stylus and etc)",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/guanghechen.git",
    "directory": "packages/postcss-modules-dts"
  },
  "homepage": "https://github.com/guanghechen/guanghechen/tree/master/packages/postcss-modules-dts#readme",
  "keywords": [
    "postcss",
    "typescript",
    "css",
    "d.ts"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@guanghechen/option-helper": "^1.0.7",
    "@types/reserved-words": "0.1.0",
    "reserved-words": "0.1.2"
  },
  "peerDependencies": {
    "@types/fs-extra": "^9.0.8",
    "fs-extra": "^9.1.0"
  }
}
