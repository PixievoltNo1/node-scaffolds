{
  "name": "@guanghechen/rollup-config",
  "version": "5.3.0",
  "description": "Rollup configs for bundle typescript project",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/node-scaffolds/tree/@guanghechen/rollup-config@5.3.0",
    "directory": "packages/rollup-config"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/@guanghechen/rollup-config@5.3.0/packages/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript"
  ],
  "type": "module",
  "module": "./lib/esm/index.mjs",
  "exports": {
    "import": "./lib/esm/index.mjs"
  },
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register script/build.ts",
    "prepublishOnly": "yarn build",
    "test": "node --experimental-vm-modules ../../node_modules/.bin/jest --config ../../jest.config.mjs --rootDir ."
  },
  "dependencies": {
    "@guanghechen/helper-npm": "^5.0.8",
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-typescript": "11.1.4",
    "import-meta-resolve": "^3.0.0",
    "rollup-plugin-dts": "^6.0.2"
  },
  "devDependencies": {
    "cross-env": "*",
    "jest": "*",
    "rimraf": "*",
    "rollup": "*",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "rollup": "^3.0.0",
    "typescript": ">=3.7.0"
  }
}
