{
  "name": "@guanghechen/rollup-config",
  "version": "4.0.0-alpha.8",
  "description": "Rollup configs for bundle typescript project",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/node-scaffolds/tree/release-4.x.x",
    "directory": "packages/rollup-config"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/release-4.x.x/packages/rollup-config#readme",
  "keywords": [
    "rollup",
    "typescript"
  ],
  "type": "module",
  "module": "./lib/esm/index.mjs",
  "exports": {
    "import": "./lib/esm/index.mjs"
  },
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "src/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib/ && cross-env NODE_ENV=production node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register script/build.ts",
    "prepublishOnly": "yarn build",
    "test": "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.mjs --rootDir ."
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "24.0.1",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@rollup/plugin-typescript": "11.0.0",
    "import-meta-resolve": "^2.2.1",
    "rollup-plugin-dts": "^5.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.4.2",
    "rimraf": "^4.1.2",
    "rollup": "^3.14.0",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "rollup": "^3.0.0"
  }
}
