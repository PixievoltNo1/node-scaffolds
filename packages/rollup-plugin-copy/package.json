{
  "name": "@guanghechen/rollup-plugin-copy",
  "version": "2.1.4",
  "description": "Rollup plugins to copy files and folds (glob supported)",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/guanghechen.git",
    "directory": "packages/rollup-plugin-copy"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/release-2.x.x/packages/rollup-plugin-copy#readme",
  "keywords": [
    "rollup",
    "rollup plugin",
    "copy"
  ],
  "type": "module",
  "exports": "./lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.js --rootDir ."
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "dir-glob": "^3.0.1",
    "fs-extra": "^11.1.0",
    "globby": "^13.1.3",
    "is-plain-object": "^5.0.0",
    "micromatch": "^4.0.5"
  },
  "peerDependencies": {
    "rollup": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/dir-glob": "^2.0.1",
    "@types/micromatch": "^4.0.2",
    "replace-in-file": "^6.3.5"
  }
}
