{
  "name": "@guanghechen/parse-lineno",
  "version": "1.9.2",
  "description": "Parse something like '1,3-10' to numbers or intervals",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/node-scaffolds.git",
    "directory": "packages/parse-lineno"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/main/packages/parse-lineno#readme",
  "keywords": [
    "lineno string",
    "interval string"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/ && tsc -p tsconfig.src.json --emitDeclarationOnly",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir ."
  },
  "devDependencies": {
    "@guanghechen/rollup-config": "^1.9.2"
  }
}
