{
  "name": "@guanghechen/helper-option",
  "version": "1.9.8",
  "description": "a collection of utility functions for processing options",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/node-scaffolds.git",
    "directory": "packages/helper-option"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/main/packages/helper-option#readme",
  "keywords": [
    "type utils",
    "string utils"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/ && tsc -p tsconfig.src.json --emitDeclarationOnly",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true jest --config ../../jest.config.js --rootDir ."
  },
  "dependencies": {
    "@guanghechen/helper-is": "^1.9.8",
    "@guanghechen/helper-string": "^1.9.8"
  },
  "devDependencies": {
    "@guanghechen/rollup-config": "^1.9.8"
  }
}
