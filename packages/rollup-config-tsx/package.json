{
  "name": "@guanghechen/rollup-config-tsx",
  "version": "3.0.0-alpha.1",
  "description": "Rollup configs for bundle typescript + react projects",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guanghechen/guanghechen.git",
    "directory": "packages/rollup-config-tsx"
  },
  "homepage": "https://github.com/guanghechen/node-scaffolds/tree/release-3.x.x/packages/rollup-config-tsx#readme",
  "keywords": [
    "rollup",
    "typescript",
    "react"
  ],
  "type": "module",
  "exports": "./lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build",
    "test": "cross-env TS_NODE_FILES=true NODE_OPTIONS=--experimental-vm-modules jest --config ../../jest.config.js --rootDir ."
  },
  "dependencies": {
    "@guanghechen/helper-is": "^3.0.0-alpha.1",
    "@guanghechen/postcss-modules-dts": "^3.0.0-alpha.1",
    "@guanghechen/rollup-config": "^3.0.0-alpha.1",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@types/postcss-url": "^10.0.0",
    "autoprefixer": "^10.4.13",
    "fs-extra": "^11.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-url": "^10.1.3",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "peerDependencies": {
    "postcss": "^8.4.16",
    "rollup": "^3.0.0"
  },
  "optionalDependencies": {
    "stylus": "^0.59.0"
  }
}
