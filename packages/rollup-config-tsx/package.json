  "dependencies": {
    "@guanghechen/helper-is": "^3.0.2",
    "@guanghechen/postcss-modules-dts": "^3.0.2",
    "@guanghechen/rollup-config": "^3.0.2",
    "@rollup/plugin-multi-entry": "^6.0.0",
    "@types/postcss-url": "^10.0.0",
    "autoprefixer": "^10.4.13",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-url": "^10.1.3",
    "rollup-plugin-postcss": "^4.0.2"
  },
  "devDependencies": {
    "@guanghechen/helper-path": "^3.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.4.1",
    "rimraf": "^4.1.2",
    "rollup": "^3.11.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.16",
    "rollup": "^3.0.0"
  },
  "optionalDependencies": {
    "stylus": "^0.59.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 35,
        "functions": 100,
        "lines": 90,
        "statements": 90
      }
    }
