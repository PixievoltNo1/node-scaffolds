{
  "__version__": "{{{lowerCase commandVersion}}}",
  "__nonce__": "{{{configNonce}}}",
  "__globalOptions__": {
    "logLevel": "{{{logLevel}}}",
    "encoding": "{{{encoding}}}",
    "plainRootDir": "{{{plainRootDir}}}",
    "cryptRootDir": "{{{cryptRootDir}}}",
    "secretFilepath": "{{{secretFilepath}}}",
    "minPasswordLength": {{{minPasswordLength}}},
    "showAsterisk": {{{showAsterisk}}}
  },
  "init": {
    "gitGpgSign": false
  },
  "encrypt": {
    "catalogCacheFilepath": ".ghc-cache-catalog.encrypt.json"
  },
  "decrypt": {
    "catalogCacheFilepath": ".ghc-cache-catalog.decrypt.json",
    "gitGpgSign": false,
    "outDir": "{{{bakPlainRootDir}}}"
  },
  "verify": {
    "catalogCacheFilepath": ".ghc-cache-catalog.encrypt.json"
  }
}
