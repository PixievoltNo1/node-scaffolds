{
  "__version__": "{{{lowerCase commandVersion}}}",
  "__nonce__": "{{{configNonce}}}",
  "__globalOptions__": {
    "logLevel": "{{{logLevel}}}",
    "encoding": "{{{encoding}}}",
    "cryptRootDir": "{{{cryptRootDir}}}",
    "minPasswordLength": {{{minPasswordLength}}},
    "plainRootDir": "{{{plainRootDir}}}",
    "secretConfigPath": "{{{secretConfigPath}}}",
    "showAsterisk": {{{showAsterisk}}}
  },
  "init": {
    "gitGpgSign": false
  },
  "encrypt": {
    "catalogCachePath": ".ghc-catalog.cache.json"
  },
  "decrypt": {
    "catalogCachePath": ".ghc-catalog.cache.json",
    "gitGpgSign": false
  },
  "verify": {
    "catalogCachePath": ".ghc-catalog.cache.json"
  }
}
